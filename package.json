    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "esbuild": "^0.19.11",
    "esbuild-jest": "^0.5.0",
    "jest": "^27.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^4.9.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.2.9"
  },
  "peerDependencies": {
    "rxjs": ">=5.5.0 || >=6.0.0 || >=7.8.1",
