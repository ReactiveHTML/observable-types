{
  "name": "observable-types",
  "version": "0.7.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "sideEffect": false,
  "scripts": {
    "start": "vite",
    "build": "rimraf dist && rollup --bundleConfigAsCjs --config rollup.config.ts --configPlugin typescript",
